/*
Count_Days_Spent_Together.cpp

*/

class Solution {
public:
    int countDaysTogether(string arriveAlice, string leaveAlice, string arriveBob, string leaveBob) {
        int aliceStartMonth = stoi(arriveAlice.substr(0,2));
        int aliceStartDate = stoi(arriveAlice.substr(3,2));
        int aliceEndMonth = stoi(leaveAlice.substr(0,2));
        int aliceEndDate = stoi(leaveAlice.substr(3,2));
    }
};